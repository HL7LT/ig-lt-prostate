Invariant: prostate-score-min-1
Description: "Score must be at least 1."
Expression: "$this >= 1"
Severity: #error

Invariant: prostate-score-max-5
Description: "Score must be at most 5."
Expression: "$this <= 5"
Severity: #error

Invariant: prostate-score-max-3
Description: "Score must be at most 3."
Expression: "$this <= 3"
Severity: #error
